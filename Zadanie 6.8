using System;

public class Prostopadloscian
{
    private double dlugosc;
    private double szerokosc;
    private double wysokosc;

    public Prostopadloscian(double dlugosc, double szerokosc, double wysokosc)
    {
        this.dlugosc = dlugosc;
        this.szerokosc = szerokosc;
        this.wysokosc = wysokosc;
    }

    public double Objetosc()
    {
        return dlugosc * szerokosc * wysokosc;
    }

    public static int PorownajObjetosci(Prostopadloscian p1, Prostopadloscian p2)
    {
        double obj1 = p1.Objetosc();
        double obj2 = p2.Objetosc();

        if (obj1 < obj2)
            return -1;
        else if (obj1 > obj2)
            return 1;
        else
            return 0;
    }
}

public class Program
{
    public static void Main()
    {
        Prostopadloscian p1 = new Prostopadloscian(2, 3, 4);
        Prostopadloscian p2 = new Prostopadloscian(3, 4, 5);

        double obj1 = p1.Objetosc();
        double obj2 = p2.Objetosc();

        Console.WriteLine("Objetosc p1: " + obj1);
        Console.WriteLine("Objetosc p2: " + obj2);

        int porownanie = Prostopadloscian.PorownajObjetosci(p1, p2);

        if (porownanie < 0)
            Console.WriteLine("Prostopadloscian p1 ma mniejsza objetosc niz p2.");
        else if (porownanie > 0)
            Console.WriteLine("Prostopadloscian p1 ma wieksza objetosc niz p2.");
        else
            Console.WriteLine("Prostopadlosciany p1 i p2 maja taka sama objetosc.");
    }
}
